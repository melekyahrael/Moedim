{
  "name": "moedim-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "mobile": "cd apps/mobile && npm start",
    "mobile:android": "cd apps/mobile && npm run android",
    "mobile:ios": "cd apps/mobile && npm run ios",
    "test": "cd apps/mobile && npm test",
    "typecheck": "cd apps/mobile && npx tsc --noEmit",
    "db:push": "npx supabase db push",
    "db:pull": "npx supabase db pull",
    "db:types": "npx supabase gen types typescript --local > apps/mobile/types/supabase.ts"
  }
}
